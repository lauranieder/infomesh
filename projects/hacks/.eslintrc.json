{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": 0,
    "no-unused-vars": [
      "warn",
      { "vars": "all", "args": "after-used", "argsIgnorePattern": "^_" }
    ],
    "no-undef": ["warn"],
    "no-proto": ["error"]
  },
  "globals": {
    "PIXI": true,
    "anime": true,
    "OPTIONS": true,
    "COLORS": true,
    "TYPES": true,
    "VW": true,
    "VH": true,
    "MorphingPath": true,
    "map_range": true,
    "gui": true,
    "dat": true
  }
}
